    "# Numpy array vs Python lists\n",
    "\n",
    "# python's lists\n",
    "list1 = [i for i in range(10000000)]\n",
    "list2 = [i for i in range(10000000,20000000)]\n",
    "\n",
    "list3 = []\n",
    "\n",
    "for i in range(len(list1)):\n",
    "    list3.append(list1[i] + list2[i])\n"
